{
  "installCommand": "corepack enable && corepack prepare pnpm@9.1.3 --activate && pnpm install --frozen-lockfile",
  "buildCommand": "pnpm run build",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://skin-node-production.up.railway.app/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
