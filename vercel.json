{
  "installCommand": "corepack enable && corepack prepare pnpm@8.15.0 --activate && pnpm install --frozen-lockfile",
  "buildCommand": "pnpm run build",
  "rewrites": [
    {"source": "/(.*)", "destination": "/"},
    {
      "source": "/api",
      "destination": "https://skin-node-production.up.railway.app/*"
    },
    {
      "source": "/ws",
      "destination": "ws://skin-node-production.up.railway.app/*"
    }
  ]
}
